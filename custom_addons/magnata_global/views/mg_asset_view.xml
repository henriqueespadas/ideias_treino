<odoo>

    <record id="view_asset_form" model="ir.ui.view">
        <field name="name">magnata.global.asset.form</field>
        <field name="model">magnata.global.asset</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="symbol"/>
                        <field name="price"/>
                        <field name="volume"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="view_asset_tree" model="ir.ui.view">
        <field name="name">magnata.global.asset.tree</field>
        <field name="model">magnata.global.asset</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="symbol"/>
                <field name="price"/>
                <field name="volume"/>
            </tree>
        </field>
    </record>

</odoo>