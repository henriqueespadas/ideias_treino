<odoo>

    <record id="view_transaction_form" model="ir.ui.view">
        <field name="name">magnata.global.transaction.form</field>
        <field name="model">magnata.global.transaction</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="player_id"/>
                        <field name="market_id"/>
                        <field name="transaction_type"/>
                        <field name="amount"/>
                        <field name="price"/>
                        <field name="date"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>





</odoo>