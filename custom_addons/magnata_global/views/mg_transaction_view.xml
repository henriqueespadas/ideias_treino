<odoo>

<record id="view_transaction_form" model="ir.ui.view">
    <field name="name">magnata.global.transaction.form</field>
    <field name="model">magnata.global.transaction</field>
    <field name="arch" type="xml">
        <form>
            <sheet>
                <group>
                    <field name="player_id"/>
                    <field name="balance_player" readonly="True"/>
                    <field name="asset_id"/>
                    <field name="market_id"/>
                    <field name="transaction_type"/>
                    <field name="amount"/>
                    <field name="price" readonly="True"/>
                    <field name="price" invisible="1"/>
                    <field name="total" readonly="True"/>
                    <field name="total" invisible="1"/>
                    <field name="date"/>
                </group>
            </sheet>
        </form>
    </field>
</record>





</odoo>