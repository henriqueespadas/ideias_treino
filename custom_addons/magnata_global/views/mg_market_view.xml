<odoo>

    <record id="view_market_form" model="ir.ui.view">
        <field name="name">magnata.global.market.form</field>
        <field name="model">magnata.global.market</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="market_type"/>
                        <field name="description"/>
                        <field name="status"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>