<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="view_csg_opponent_tree" model="ir.ui.view">
        <field name="name">csg.opponent.tree</field>
        <field name="model">csg.opponent</field>
        <field name="arch" type="xml">
            <tree string="Oponentes">
                <field name="opponent_id"/>
                <field name="acronym"/>
                <field name="image_url"/>
                <field name="location"/>
                <field name="name"/>
                <field name="slug"/>
                <field name="type"/>
            </tree>
        </field>
    </record>
</odoo>
